{
  "functions": {
    "source": "functions",
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ]
  },
  "emulators": {
    "apphosting": {
      "port": 5007,
      "rootDirectory": "./",
      "startCommand": "npm run dev:next"
    },
    "auth": {
      "port": 9097
    },
    "functions": {
      "port": 5007
    },
    "firestore": {
      "port": 8087
    },
    "storage": {
      "port": 9197
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  },
  "storage": {
    "rules": "storage.rules"
  },
  "firestore": {
    "rules": "firestore.rules"
  }
}
